@model AplombTech.WMS.QueryModel.Reports.MotorOnOff
<table id="onOffSummeryTable" class="display">
    <thead>
        <tr>

            <th>
                <h3>On Time</h3>
            </th>
            <th>
                <h3>Off Time</h3>
            </th>
            <th>
                <h3>Duration</h3>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var summaryData in Model.MotorOnOffDatas)
        {
            <tr>
                <td>@summaryData.OnDateTime</td>
                <td>@summaryData.OffDateTime</td>
                <td>@summaryData.Duration</td>
            </tr>
        }
    </tbody>
</table>


<script>
    $(document).ready(function () {
        $('#onOffSummeryTable').DataTable();
    });
</script>